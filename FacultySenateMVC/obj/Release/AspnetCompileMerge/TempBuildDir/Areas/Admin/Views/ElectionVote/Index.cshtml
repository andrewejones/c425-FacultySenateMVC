@model IEnumerable<FacultySenateMVC.Models.ElectionVote>

@{
    ViewBag.Title = "Election Votes";
}

<h2>Election Votes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            Election Id
        </th>
        <th>
            Committee
        </th>
        <th>
            Nominee
        </th>
        <th>
            Voter
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Election.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Committee.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty_Nominee.First) @Html.DisplayFor(modelItem => item.Faculty_Nominee.Last)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty_Voter.First) @Html.DisplayFor(modelItem => item.Faculty_Voter.Last)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
